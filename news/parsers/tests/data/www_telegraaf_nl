      var rankingsecties20825089 = new Array();
  
      function rankingselect20825089(keus) {
        for (x in rankingsecties20825089) {
          var elem = document.getElementById("ranking"+rankingsecties20825089[x]+"20825089");
          var button = document.getElementById("rankingbut"+rankingsecties20825089[x]+"20825089");
    
    if (elem != null){
           if (rankingsecties20825089[x]==keus) {
            elem.style.display = "block"; button.style.color = "#000000";
           } else {
            elem.style.display = "none"; button.style.color = "#006699";
           }
          }
        }
      }
  </script>
