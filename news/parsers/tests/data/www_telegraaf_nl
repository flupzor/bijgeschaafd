			var sectionPath = path.substring(0, path.lastIndexOf('/')),
				sections = sectionPath.split('/'),
				result = '';

			for(var section in sections) {
				result += sections[section].replace('_', '').replace('-', '');
				if (section < sections.length - 1 && section > 0) {
					result += '_';
				}
			}

			return result === '' ? 'homepage' : result;
		};

		var getGAChannelNameForPath = function(path) {
			// Helper function for Tealium

			var parts = path.split('/');
			parts.pop();
			if (parts.length === 0) {
				return '';
			}
			if (parts[1] === 'tv' || parts[1] === 'premium') {
				return parts[2];
			} else {
				return parts[1];
			}
		};

		function getGAPageTypeForPath(path) {
			var res = 'unknown',
				isTV = (path.indexOf('/tv/') > -1);

			path = path.toLowerCase();

			if (artid && ! isTV) {
				if (teaserBannerBlock) {
					res = 'teaser';
				} else if (path.indexOf('/premium/') > -1) {
					res = 'premiumartikel';
				} else {
					res = 'artikelpagina';
				}
			} else {
				if (path === '/' || path === '/index.jsp') {
					res = 'homepage';
				} else if (isTV) {
					res = 'videopagina';
				} else if (path.indexOf('/zoeken/') > -1) {
					res = 'zoekresultaten';
				} else if (path.indexOf('/tag/') > -1) {
					res = 'tagpagina';
				} else if (path.indexOf('/koersen/') > -1) {
					res = 'koersenpagina';
				} else {
					res = 'sectiepagina';
				}
			}
			return res;
		}
		// END LEGACY FUNCTIONS

		function getPath(path){
			return path.substr(0, path.lastIndexOf("/"));

		}

		//nobo functions
		function getSection(path) {

			var path = path.toLowerCase();
			var temp=path.split("/");

			if (temp[1] === 'tv' || temp[1] === 'premium') {
				if (temp[2] === undefined || temp[2] === null || temp[2] === "") {
				    return 'empty';
				}
				return temp[2];
			} else {
				if (temp[1] === undefined || temp[1] === null || temp[1] === "") {
				    return 'empty';
				}
				return temp[1];

			}
			return 'empty';


		}
		function getSubSection(path) {

			var path = path.toLowerCase();
			var temp=path.split("/");

			if (temp[1] === 'tv' || temp[1] === 'premium') {
				if (temp[3] === undefined || temp[3] === null || temp[3] === "") {
				    return 'empty';
				}
				return temp[3];
			} else {
				if (temp[2] === undefined || temp[2] === null || temp[2] === "") {
				    return 'empty';
				}
				return temp[2];

			}
			return 'empty';
		}
		function isPremium(path) {

			var path = path.toLowerCase();
			var res = 'n';
			if (artid && artid>0) {
				if (path.indexOf('/premium/') > -1) {
					res = 'y';
				}
			}
			return res;

		}
		function getKind(path) {

			var path = path.toLowerCase();
	 		var res = 'index';
			if (artid && artid>0 ) {
				res = 'article';
			}
			return res;
		}

		function isUserLogged() {

			var res = 'n';
			if (tmgcookie && tmgcookie['user_name']) {
				res = 'y';
			}
			return res;

		}
		function isPayingUser() {

			var res = 'n';
			if (tmgcookie && tmgcookie['ga_delv']) {
				res = 'y';
			}
			return res;

		}

		function getPubDate() {

			var pubDate = '';
			var t=pubDate.split(" ")[0].split("-");
			var temp=new Date(t[0],t[1],t[2]);
			var res='';
			if(temp)
				res=''+temp.getFullYear()+('0' + temp.getDate()).slice(-2)  + ('0' + (temp.getMonth())).slice(-2) ;

			if (res === undefined || res === null || res !== res || res === 'NaNaNaN' || res === "") {
			    return 'empty';
			}
			return res;

		}




		//end nobo functions

		var userData = JSON.parse(Cookies.get("TMGCOOKIE") || "{}"),
			probability = Cookies.get('TMGCOOKIE.probability'),
			osicontext = osicontext || {},
			gallery = gallery || undefined,
			adTechKeywords = [],
			pageName = getAdtechPageNameForPath(window.location.pathname),
			articleId = window.artid,
			adPageName;

		if (userData.hasOwnProperty('user_dateofbirth')) {
			userData.age = Math.floor((new Date() - new Date(userData.user_dateofbirth*1000))/(365*24*60*60*1000)) || -1;
		}
		userData.probability = probability;

		if (articleId != 0) {
			adPageName = pageName.substring(0, pageName.lastIndexOf(articleId) - 1);
		} else {
			adPageName = pageName;
		}

		var utagData = {

			// information about the page
			'page_section_name': getGAChannelNameForPath(window.location.pathname),

			// information about the visitor
			// visitor_has_premium: store.get('has_premium'),
			// visitor_gender: store.get('gender'),

			'privacy_consent_level': consent_level(),

			'article_id': articleId,

			'metrics_page_type': getGAPageTypeForPath(window.location.pathname),
			'metrics_visit_type': ('ga_delv' in userData ? userData['ga_delv'] : 'niet ingelogd'),
			'metrics_abo_vorm': ('ga_form' in userData ? userData['ga_form'] : 'niet ingelogd'),

			'user': userData,
			'ad_revenuescience_tags': {
				'artid': articleId,
				'age': userData.age || probability,
				'gender': userData.user_gender
			},
			'ad_site_name': 'telegraaf',
			'ad_selector': '.tg-ads-placeholder',
			'ad_channel_name': adPageName,
			'ad_adtech_fallback_ids': { // adtech-identifier (fallback might be 4610536)
				'728x90': window.adtech728x90,
				'336x280': window.adtech336x280,
				'300x250': window.adtech300x250
			},
			'ad_tech_reload' : false,
            'ad_tech_reloadInterval' : "180000",
			nobo: {
			    media : "tmg",
			    brand : "tmnl",
			    platform : "web",
			    name : "telegraaf.nl",
			    date : getPubDate(), //{yyyyddmm} publishing date
			    pcontent : isPremium(getPath(window.location.pathname)), //{y/n} is the articles a premium article
			    puser : isPayingUser(), //{y/n} did the user pay for this article
			    login : isUserLogged(), //{y/n} is the user logged in
			    primbrand : "telegraaf",
			    secbrand : getSection(getPath(window.location.pathname)), //"{sectionname}" //is it a dft article or vrouw, etc...
			    genre : getSubSection(getPath(window.location.pathname)), //{subsection} dft geld, vrouw horoscopen, etc...
			    ptype : getKind(getPath(window.location.pathname)), //{article | index} is it an article page or an index page
			    mtype : "non-replica", //is it a replica page or not
			}

		};

		if (osicontext) {
			$.each(['osiCountry', 'osiRegion', 'cityOsi'], function(i){
				if (i in osicontext) {
					utagData.ad_revenuescience_tags[i] = osicontext[i];
				}
			});
		}

		if (gallery != 'null') { // add custom 'gallery' parameter if present in URL
			adTechKeywords.push(gallery);
		}
		if (typeof teaserBannerBlock != 'undefined' && teaserBannerBlock) {
			adTechKeywords.push('premium');
		}
		utagData.ad_adtech_tags = {
			custom: ';kw=' + adTechKeywords.join(';kw=') + ';'
		}

		window['utag_data'] = utagData;

	})();
</script>
