   <!--  Realtime data targetting  -->
   <!-- Data pilot -->
    <script type="text/javascript">

/**
 * [getCookie Helper function to get parsed cookie data]
 * @param  {[type]} sKey [cookie name]
 * @return {[type]}      [string]
 */
function getCookie(sKey) {
    if (!sKey) { return null; }
    return decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent(sKey).replace(/[\-\.\+\*]/g, "\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")) || null;
}

/**
 * [placePixel This callback is called when the jsonp call returns
 *             with the json data wrapped inside it. The data is then
 *             appended in the pixel as params and the pixel is
 *             inserted dynamically into the page.      ]
 * @param  {[type]} data [json]
 * @return {[type]}      [none]
 */
function placePixel(data) {
    if (data !== undefined && data != "") {
        var imgElem = document.createElement('img');
        var src = "http://pix04.revsci.net/G07609/b3/0/3/noscript.gif?D=";
        var fragment = "http://www.telegraaf.nl?DM_EOM=1&C=G07609&L=0";
        var paramStr = "";
        for (var key in data) {
            paramStr += "&" + key + "=" + data[key];
        }
        if (paramStr.length > 0) {  //ensure that some data was added only then append the pixel
            src += encodeURIComponent("DM_LOC=" + encodeURIComponent(fragment + paramStr));

            imgElem.src = src;
            imgElem.width = '1';
            imgElem.height = '1';
            imgElem.style.display = "none";

            document.head.appendChild(imgElem);
        }
    }
}

/**
 * Start the process by fetching the cookie and appending the script to fetch
 * the JSON data from our servers using JSONP.
 */
var tmgCookie = JSON.parse(getCookie('TMGCOOKIE'));

if (tmgCookie !== undefined && tmgCookie !== null && tmgCookie.user_uid) {
    var userId = tmgCookie.user_uid;

    script = document.createElement('script');
    script.type = "text/javascript";
    script.src = "http://dd.tnet.nl/as/ui/?uid=" + userId + "&callback=placePixel";

    document.head.appendChild(script);
}

    </script>
</body>

</html>

