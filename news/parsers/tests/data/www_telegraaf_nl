				var refreshtimer = 0;
				var refreshtimeout = 900;

				function refreshinit() {
					refreshtimer = setTimeout(pagerefresh,
							refreshtimeout * 1000);
					setTimeout(function() {
						tmgEvent("content", "refresh");
					}, (refreshtimeout - 2) * 1000);
				}

				function pagerefresh() {
					document.location.href = document.location.href;
				}

				if (window.addEventListener)
					window.addEventListener("load", refreshinit, false);
				else if (window.attachEvent)
					window.attachEvent("onload", refreshinit);

				/* break us out of any containing iframes */
				if (top != self
						&& !(window.location.hostname.indexOf("gerichtonline.") > -1)
						&& !(window.location.hostname.indexOf("wumoo.nl") > -1)) {
					top.location.replace(self.location.href);
