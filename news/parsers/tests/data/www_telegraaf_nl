                $(function() {
                    $("#pagination23812303").pagination(15, {
                        items_per_page:6,
                        num_display_entries:11,
                        current_page:0,
                        num_edge_entries:0,
                        link_to:"#",
                        prev_text:"<",
                        next_text:">",
                        ellipse_text:"&hellip;",
                        prev_show_always:true,
                        next_show_always:true,
                        load_first_page:false,
                        callback:loadVideosCallback23812303
                    });
                    function loaddefault() {
                        return false;
                    }
                    function loadVideosCallback23812303(page_index){
                        var items_per_page = 6;
                        //var max_elem = Math.min((page_index+1) * items_per_page, videoPaginationRow23812303.length);

                        $.ajax({ url: "/jsp/video_carrousel_page.jsp?artid=23812303&pagenr=" + (page_index+1) }).done(function( data ) {
                          $('#videoRow23812303 .videoRowWrapper').html(data);

                          // Hide images when switching pages, then fade images in when loaded
                    $("#videoRow23812303 .videoItem div.videoOverlay img").bind("load", function () { $(this).fadeIn(800); }).hide();

                      // Add mouseover events to thumbnails in row individually
                            $('#videoRow23812303 .videoItem').on('mouseenter', function() {
                              $(this).children("div.videoOverlay").addClass('hoverBorder');
                            });
                            $('#videoRow23812303 .videoItem').on('mouseleave', function() {
                              $(this).children("div.videoOverlay").removeClass('hoverBorder');
                            });
                    });
                        // Prevent click eventpropagation
                        return false;
                    }

                });
            </script>
